# -*- mode: conf -*-

[credentials]
username=<%= scope.lookupvar('cloudmonitoring::username') %>
api_key=<%= scope.lookupvar('cloudmonitoring::apikey') %>

[api]
url=<%= scope.lookupvar('cloudmonitoring::api_url') %>

[auth_api]
url=<%= scope.lookupvar('cloudmonitoring::auth_api_url') %>

[ssl]
verify=true
